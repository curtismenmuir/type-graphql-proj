{
  "name": "type-graphql-proj",
  "version": "1.0.0",
  "private": true,
  "description": "Project to test using the Type-GraphQL framework from creating a TypeScript GraphQL server implementation",
  "main": "index.ts",
  "scripts": {
    "start": "npm run-script build && node 'dist/index.js'",
    "build": "rm -rf ./dist && mkdir dist && tsc -p . && cp secrets.json dist",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon 'index.ts' --exec 'ts-node' index.ts -e ts,graphql"
  },
  "keywords": [
    "Type-GraphQL",
    "TypeScript",
    "JavaScript",
    "Apollo-Server",
    "GraphQL"
  ],
  "author": "Curtis Menmuir",
  "license": "MIT",
  "dependencies": {
    "@types/graphql": "^14.2.0",
    "apollo-server": "^2.4.8",
    "class-transformer": "^0.2.0",
    "graphql": "^14.2.1",
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^1.18.11",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^8.0.3",
    "type-graphql": "^0.17.1",
    "typescript": "^3.4.3"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^8.3.2"
  }
}
